
@using LibraryManagementLibrary.Models
@model List<Address>

@{

}


@{
    foreach (var addressItem in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(ai => addressItem.Number)
            </td>
            <td>
                @Html.DisplayFor(ai => addressItem.AddressLine)
            </td>
            <td>
                @Html.DisplayFor(ai => addressItem.Town)
            </td>
            <td>
                @Html.DisplayFor(ai => addressItem.Postcode)
            </td>
        </tr>
    }
}

